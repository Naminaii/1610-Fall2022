using System;

public class Program
{
public Hero HeroOne;
public Hero HeroTwo;

public void main()
{
heroOne = new Hero();
heroTwo = new Hero();

heroOne.health = 5;
heroOne.powerLevel = 5;

heroTwo.health = 3;
heroTwo.powerLevel = 4;

Console.WriteLine(heroOne.health);
Console.WriteLine(heroOne.powerLevel);

Console.WriteLine(heroTwo.health);
Console.WriteLine(heroTwo.powerLevel);

int hurtHeroOne = heroOne.health -= 3;
int newPowerTwo = heroTwo.powerLevel *= 2;
Console.WriteLine("Oh no! Hero One took damage. Health at:" hurtHeroOne);
Console.WriteLine("Hero Two just found a power up. Poggers! Power level now at:" newPowerTwo);
}

public class Hero()
{
public int health;
public int powerLevel;
}
}

using System;
					
public class HelloWorld
{
	public static void Main(string[] args)
	{
		float TroubleNum = 17.68;
    float Pie = 3.141592353;
    int SweetMercy = 374 *= 15;
    !(SweetMercy >= Pie && Pie <= TroubleNum);
    Console.WriteLine(SweetMercy %= Pie);
	}
}
