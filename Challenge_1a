public class ClassWithVariables
{
public int Penumber = 419;
public int Nightfail = 215;
public string userName = "Juno Steel";

console.WriteLine(Penumber += Nightfail);
double uncomfyNum = 3.45D;

static void VeryUncomfyMath()
{
long badNum = 27000000L;
float OhWhy = badNum /= uncomfyNum;
int NotAsBad = Penumber *= Nightfail;
float Suffering = OhWhy ^= 32;

Console.WriteLine(Suffering -= NotAsBad);
}
}

public class ClassWithFunctions
{
public void RunFuction()
{
console.WriteLine("What was going on with the function up there?  Like....someone has math trauma, amirite?");
}
}

